    <RmHLSLShader NAME="Vertex Shader" API="D3D" PIXEL_SHADER="FALSE" TARGET="vs_2_0" ENTRY_POINT="vs_main" COMPILER_FLAGS="0"><RmShaderCode TYPE="TEXT"><![CDATA[
float4x4 matViewProjection;

struct VS_INPUT 
{
   float4 Position : POSITION0;
   float2 Texcoord : TEXCOORD0;
   float3 Normal : NORMAL0;
};

struct VS_OUTPUT 
{
   float4 Position : POSITION0;
   float2 Texcoord : TEXCOORD0;
   
};

float3 scale, scale2, frontScale, scale3;

float4 mx1, mx2, mx3, mx4, mx5;

#include "utils.fx"

VS_OUTPUT vs_main( VS_INPUT Input )
{
   VS_OUTPUT Output;

   float4 inPos = Input.Position;
   inPos.xyz *= scale;
   
   inPos.y -= 35 * Input.Normal *
      smoothstep(inPos.y,mx1.x, mx1.y); 

   if(inPos.y > -10 && inPos.y < 20)
   {
      if(inPos.x < -10)
      {
         inPos.x -= 10;
      }
   }
   
   inPos.xyz += Input.Normal * frontScale * 
            smoothsel(mx2.x,mx2.y,mx2.z,mx2.w,length(inPos.x)) * 
            smoothsel(mx3.x,mx3.y,mx3.z,mx3.w,length(inPos.y));
   
   inPos.xyz += Input.Normal * scale2 * smoothsel(mx4.x, mx4.y, mx4.z, mx4.w, inPos.y);
   
   inPos.xyz += Input.Normal * scale3 * smoothsel(mx5.x, mx5.y, mx5.z, mx5.w, inPos.y);

   Output.Position = mul( inPos, matViewProjection );
   Output.Texcoord = Input.Texcoord;

   return( Output );
   
}



]]></RmShaderCode>

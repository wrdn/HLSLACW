    <RmHLSLShader NAME="Vertex Shader" API="D3D" PIXEL_SHADER="FALSE" TARGET="vs_3_0" ENTRY_POINT="vs_main" COMPILER_FLAGS="0"><RmShaderCode TYPE="TEXT"><![CDATA[
float3 lightPos;
float4 eyePos;

struct VS_INPUT
{
   float4 Position : POSITION0;
   float2 Texcoord : TEXCOORD0;
   float3 Normal : NORMAL0;
};

struct VS_OUTPUT
{
   float4 Position : POSITION0;
   float2 Texcoord : TEXCOORD0;
   float3 ViewDirection :   TEXCOORD1;
   float3 LightDirection :  TEXCOORD2;
   float3 Normal :          TEXCOORD3;
   float4 WorldPos : TEXCOORD4;
};

float4x4 matView, matViewProjection;

float time;

float min1, max1, min2, max2, min3,max3,min4,max4, min5, max5, min6, max6;

float spoutCutoff, spoutCutoff2;

float3 translation;

float3 spoutScale;
float3 handleScale;
float3 sideScale;
float3 vehicleScale;

#include "utils.fx"

VS_OUTPUT vs_main( VS_INPUT Input )
{

   VS_OUTPUT Output = (VS_OUTPUT)0;
   float4 pos = Input.Position;
   
   // DO VERTEX DEFORMATIONS HERE
   float Lx = length(pos.x);
   float Ly = length(pos.y);
   float Lz = length(pos.z);
   
   if(pos.x > spoutCutoff)
   {
      // remove spout
      pos = float4(0,0,0,0);
   }
   else
   {
      // rescale end of spot
      if(pos.x < spoutCutoff && pos.x > spoutCutoff - 2.5)
      {
         pos.xyz *= spoutScale;
      }
      
      // straighten back
      if ( pos.x < -30 && pos.x >-50 )      
      {
         pos.x = -30; 
      }
      if ( pos.z > 30 && pos.z <80 )
      {
         pos.z = 30;
      }
      else if ( pos.z < -30 && pos.z >-80 )
      { 
         pos.z = -30; 
      }
      
      
      if(pos.y > 30 && pos.x > -30)
      {
         pos.xyz *= handleScale;
      }
      
      // handle
      if (pos.x < -30)
      {
         pos.xy += 25;
      }
      
      // sides
      pos.xyz += Input.Normal * sideScale * 
           smoothsel(min3,max3,min4,max4,Lx) * smoothsel(min5,max5,min6,max6,Ly);
           
      // bottom
      if(
         (pos.z > -30 && pos.z < -10) ||
         (pos.z > 10 && pos.z < 40))
      {
         if(pos.y < -38)
         {
            pos.y -= 15;
         }
      }
   }
   
   
   Output.WorldPos = pos;
   
   // END OF VERTEX DEFORMATIONS
   
   // Scaling
   pos.xyz *= vehicleScale;
   
    // Translation
   pos.xyz += translation;
   
   Output.Position = mul( pos, matViewProjection );
   Output.Texcoord = Input.Texcoord;
   
   float3 objectPos = mul(pos.xyz, matView);
   Output.LightDirection = normalize(lightPos - objectPos);
   Output.ViewDirection = normalize(eyePos.xyz - objectPos);
   Output.Normal = normalize(mul(Input.Normal, matView));
   
   return( Output );
   
}



]]></RmShaderCode>

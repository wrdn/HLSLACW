    <RmHLSLShader NAME="Pixel Shader" API="D3D" PIXEL_SHADER="TRUE" TARGET="ps_3_0" ENTRY_POINT="ps_main" COMPILER_FLAGS="0"><RmShaderCode TYPE="TEXT"><![CDATA[
float4 ka, kd, ks;
float specularPower;
sampler2D tex, tex2;
samplerCUBE cubeMap;

struct PS_INPUT
{
   float2 Texcoord : TEXCOORD0;
   float3 ViewDirection :   TEXCOORD1;
   float3 LightDirection :  TEXCOORD2;
   float3 Normal :          TEXCOORD3;
   float4 WorldPos : TEXCOORD4;
};

float2 texMix;

float4 ps_main( PS_INPUT Input ) : COLOR0
{
   float3 lightDir = normalize(Input.LightDirection);
   float3 normal = normalize(Input.Normal);
   float NdotL = dot(normal, lightDir);
   
   float3 R = normalize( ((2.0f*normal) * NdotL) - lightDir);
   float3 V = normalize(Input.ViewDirection);
   float RdotV = max(0.0f, dot(R,V));
   
   
   float4 baseColor;
   if(Input.WorldPos.y < -38)
   {
      Input.Texcoord = float2( Input.Texcoord.y, Input.Texcoord.x);
      baseColor = tex2D(tex2, Input.Texcoord*15);
   }
   else
   {
      baseColor = tex2D(tex, Input.Texcoord);
   }
   
   //float4 baseColor = texMix.x*tex2D(tex, Input.Texcoord) + texMix.y*tex2D(tex2, Input.Texcoord);
   
   //float3 reflectedDir = reflect(Input.ViewDirection, Input.Normal);
   //baseColor += texCUBE(cubeMap, reflectedDir);
   
   float4 fvTotalAmbient   = ka * baseColor; 
   float4 fvTotalDiffuse   = kd * NdotL * baseColor; 
   float4 fvTotalSpecular  = ks * pow( RdotV, specularPower );
   
   return( saturate( fvTotalAmbient + fvTotalDiffuse + fvTotalSpecular ) );
}



]]></RmShaderCode>

     <RmHLSLShader NAME="Vertex Shader" API="D3D" PIXEL_SHADER="FALSE" TARGET="vs_3_0" ENTRY_POINT="vs_main" COMPILER_FLAGS="0"><RmShaderCode TYPE="TEXT"><![CDATA[
float3 lightPos, eyePos, translation;
float4x4 matView, matWorld, matViewProjection;
float maxHeight;

texture displacementMap;
sampler displacementSampler = sampler_state
{
        Texture = <displacementMap>;
        MipFilter = Point;
        MinFilter = Point;
        MagFilter = Point;
        AddressU = Clamp;
        AddressV = Clamp;
};

struct VS_INPUT 
{
   float4 Position : POSITION0;
   float2 Texcoord : TEXCOORD0;
   float3 Normal :   NORMAL0;
   
};

struct VS_OUTPUT 
{
   float4 Position :        POSITION0;
   float2 Texcoord :        TEXCOORD0;
   float3 ViewDirection :   TEXCOORD1;
   float3 LightDirection :  TEXCOORD2;
   float3 Normal :          TEXCOORD3;
   float4 worldPos : TEXCOORD4;
   float4 textureWeights : TEXCOORD5;
};

#include "utils.fx"

float dimension, cellSize;

float4 texWeight;
float3 scale;
float minY, maxX;

VS_OUTPUT vs_main( VS_INPUT Input )
{
   VS_OUTPUT Output = (VS_OUTPUT)0;
   
   float height = tex2Dlod(displacementSampler, float4(Input.Texcoord, 0,0));
   height = tex2Dlod_bilinear(displacementSampler,
      float4(Input.Texcoord.xy, 0,0), dimension, 1.0/dimension);
   
   float4 inPos = float4(Input.Position.xy,0,1);
   inPos = mul(inPos, build_rotation_matrix(90,1,0,0));
   inPos.y = height * maxHeight;
   
   inPos.y = max(minY, inPos.y);
   //inPos.x = min(maxX, inPos.x);
   
   inPos.xyz *= scale;
   
   //inPos.xyz += translation;
   
   Output.worldPos = mul(inPos, matWorld);
   
   Output.Position = mul(inPos, matViewProjection);
   
   Output.Texcoord         = Input.Texcoord;
   
   float3 objectPos = mul(inPos.xyz, matView);
   Output.LightDirection = lightPos - objectPos;
   
   Output.ViewDirection = eyePos - objectPos;
   
   float4 TexWeights = 0;
   TexWeights.x = saturate(1.0f - abs(height - texWeight.x) / 0.2f);
   TexWeights.y = saturate(1.0f - abs(height - texWeight.y) / 0.25f);
   TexWeights.z = saturate(1.0f - abs(height - texWeight.z) / 0.25f);
   TexWeights.w = saturate(1.0f - abs(height - texWeight.w) / 0.25f);
   TexWeights /= (TexWeights.x + TexWeights.y + TexWeights.z + TexWeights.w);
   Output.textureWeights = TexWeights;
   
   return Output; 
}


]]></RmShaderCode>

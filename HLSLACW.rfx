     <RmHLSLShader NAME="Pixel Shader" API="D3D" PIXEL_SHADER="TRUE" TARGET="ps_3_0" ENTRY_POINT="ps_main" COMPILER_FLAGS="0"><RmShaderCode TYPE="TEXT"><![CDATA[
float4 ka, kd, ks;
float specularPower;
float maxHeight;

sampler2D displacementNormalMap;

texture grass;
sampler grassSampler = sampler_state
{
    Texture   = <grass>;
    MipFilter = Linear;
    MinFilter = Linear;
    MagFilter = Linear;
    AddressU  = Wrap;
    AddressV  = Wrap;
};
texture sand;
sampler sandSampler = sampler_state
{
    Texture   = <sand>;
    MipFilter = Linear;
    MinFilter = Linear;
    MagFilter = Linear;
    AddressU  = Wrap;
    AddressV  = Wrap;
};
texture rock;
sampler rockSampler = sampler_state
{
    Texture   = <rock>;
    MipFilter = Linear;
    MinFilter = Linear;
    MagFilter = Linear;
    AddressU  = Wrap;
    AddressV  = Wrap;
};
texture snow;
sampler snowSampler = sampler_state
{
    Texture   = <snow>;
    MipFilter = Linear;
    MinFilter = Linear;
    MagFilter = Linear;
    AddressU  = Wrap;
    AddressV  = Wrap;
};

struct PS_INPUT 
{
   float2 Texcoord :        TEXCOORD0;
   float3 ViewDirection :   TEXCOORD1;
   float3 LightDirection:   TEXCOORD2;
   float3 Normal :          TEXCOORD3;
   float4 worldPos : TEXCOORD4;
   float4 textureWeights : TEXCOORD5;
};

float texScale;
float4 ps_main( PS_INPUT Input ) : COLOR0
{
   //return Input.worldPos.y / maxHeight;

   float4 disNM = tex2D(displacementNormalMap, Input.Texcoord);

   float3 fvLightDirection = normalize( Input.LightDirection );
   float3 fvNormal         = normalize( disNM );
   float  fNDotL           = dot( fvNormal, fvLightDirection ); 
   
   float3 fvReflection     = normalize( ( ( 2.0f * fvNormal ) * ( fNDotL ) ) - fvLightDirection ); 
   float3 fvViewDirection  = normalize( Input.ViewDirection );
   float  fRDotV           = max( 0.0f, dot( fvReflection, fvViewDirection ) );
   
   float4 sand = tex2D(sandSampler, Input.Texcoord * texScale);
   float4 grass = tex2D(grassSampler, Input.Texcoord * texScale);
   float4 rock = tex2D(rockSampler, Input.Texcoord * texScale);
   float4 snow = tex2D(snowSampler, Input.Texcoord * texScale);
   
   float4 w = Input.textureWeights;
   float4 fvBaseColor = sand*w.x + grass*w.y + rock*w.z + snow*w.w;
   
   
   float4 fvTotalAmbient   = ka * fvBaseColor; 
   float4 fvTotalDiffuse   = kd * fNDotL * fvBaseColor; 
   float4 fvTotalSpecular  = ks * pow( fRDotV, specularPower );
   
   return( saturate( fvTotalAmbient + fvTotalDiffuse + fvTotalSpecular ) );
      
}


]]></RmShaderCode>

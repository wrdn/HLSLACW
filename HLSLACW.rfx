sampler2D displacementMap, rockMap, snowMap, sandMap, grassMap;
float normalStrength;

#include "utils.fx"

struct PS_OUTPUT
{
   float4 firstColor : COLOR0;
   float4 secondColor : COLOR1;
   float4 thirdColor : COLOR2;
   float4 fourthColor : COLOR3;
};

PS_OUTPUT ps_main(float2 uv : TEXCOORD0) : COLOR0
{
    PS_OUTPUT outp = (PS_OUTPUT)0;
    outp.firstColor = GenerateNormalMap(displacementMap, uv, 512, normalStrength);
    outp.secondColor = GenerateNormalMap(rockMap, uv, 512, normalStrength);
    outp.thirdColor = GenerateNormalMap(snowMap, uv, 512, normalStrength);
    outp.fourthColor = GenerateNormalMap(sandMap, uv, 512, normalStrength);
    
    return outp;
}]]></RmShaderCode>
